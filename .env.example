# Root Environment Variables
NODE_ENV=development

# Database Configuration
DATABASE_URL=mongodb://localhost:27017/orbital-ai-explorer
# Or for PostgreSQL: postgresql://username:password@localhost:5432/orbital_ai_explorer

# API Keys
NASA_API_KEY=your_nasa_api_key_here
SPACEX_API_URL=https://api.spacexdata.com/v4
OPENAI_API_KEY=your_openai_api_key_here

# Authentication
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# OAuth (optional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Server Configuration
CLIENT_PORT=3000
SERVER_PORT=3001
AI_SERVICE_PORT=8001

# CORS
ALLOWED_ORIGINS=http://localhost:3000

# AI Service Configuration
AI_MODEL_TYPE=openai
# Options: openai, local, huggingface
LOCAL_MODEL_PATH=./models/your_model
HUGGINGFACE_API_KEY=your_huggingface_key
